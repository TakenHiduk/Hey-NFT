{
  "name": "hey-nft",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENCED",
  "scripts": {
    "start": "node \"dist/index.js\"",
    "dev": "svelte-kit dev",
    "prebuild": "rimraf .svelte-kit dist",
    "clean": "rimraf yarn.lock node_modules .svelte-kit dist",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "prelint": "prettier --check \"src/**/*.{html,svelte,ts}\"",
    "lint": "eslint \"src/**/*.{svelte,ts}\"",
    "lint:fix": "eslint \"src/**/*.{svelte,ts}\" --fix",
    "format": "prettier --write \"src/**/*.{html,svelte,ts}\"",
    "upgrade": "yarn upgrade-interactive --latest"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "1.0.0-next.66",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "autoprefixer": "10.4.2",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-typescript-airbnb-prettier-svelte": "1.1.2",
    "eslint-plugin-svelte3": "3.4.0",
    "postcss": "8.4.5",
    "prettier": "2.5.1",
    "prettier-plugin-svelte": "2.6.0",
    "rimraf": "3.0.2",
    "sass": "1.49.0",
    "svelte": "3.46.2",
    "svelte-check": "2.3.0",
    "svelte-preprocess": "4.10.2",
    "tailwindcss": "3.0.15",
    "tslib": "2.3.1",
    "typescript": "4.5.5"
  },
  "type": "module",
  "dependencies": {}
}
